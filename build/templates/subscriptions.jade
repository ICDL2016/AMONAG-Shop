extends layouts/layout-logged
block title
    a.page-title(href='') Подписки
block content
    .container-index
        .catalog.catalog-other
            .catalog__item
                a(href="#").catalog__img-wrap
                    img(src="assets/i/catalog1.png")
                .catalog__text
                    .catalog__languages
                        span RU
                        span EN
                        span ES
                    a(href="") Заголовок анонса материала, содержащий до 250 символов
                .catalog__date.text-bold 10.10.2014 — 09.10.2014

                .catalog__price
                    .catalog__price-left
                        .catalog__price-old 6000
                            i.fa.fa-rub
                        .catalog__price-current 3000
                            .fa.fa-rub
                    .catalog__price-right
                        a.fa.fa-folder-o(href="",title="Добавить в избранное")

                a.btn.btn-lg.btn-block(href='') Продлить
